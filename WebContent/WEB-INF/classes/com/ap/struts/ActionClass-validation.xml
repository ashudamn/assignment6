<?xml version="1.0" encoding="UTF-8"?>
<validators>
	<field name="username">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>required</message>
		</field-validator>
	</field>
	<field name="query">
		<field-validator type="requiredstring">
			<message>required</message>
		</field-validator>
	</field>
	<field name="password">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>required</message>
		</field-validator>
	</field>
</validators>